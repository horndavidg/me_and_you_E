      </div>

      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <% if (currentuser == null) { %>
          <li><a href="/login"><span class="white">Login</span></a></li>
          <% } %>

        </ul>
     <ul class="nav navbar-nav navbar-right">
         <% if (currentuser != null) { %>

<li><p class="navbar-text"><span class="white">Signed in as <a href="/users/<%=currentuser._id%>" class="navbar-link"><%= currentuser.email %></a></span></p></li>
<li><a href="/logout"><span class="white">Logout</span></a></li>


          <% } %>
        </ul>
      </div>
    </div>
